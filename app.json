{
  "expo": {
    "name": "GYM",
    "slug": "gym",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gym2",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.manideep1428.gymupdated",
      "infoPlist": {
        "UIBackgroundModes": [
          "background-fetch",
          "remote-notification"
        ],
        "NSUserNotificationsUsageDescription": "This app uses notifications to keep you updated about booking confirmations, session reminders, and trainer requests."
      },
      "notification": {
        "icon": "./assets/images/icon.png",
        "sound": "./assets/notifications.mp3"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff",
          "mode": "production",
          "sounds": [
            "./assets/notifications.mp3"
          ]
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.446928845689-b6ml0pbcdv0a0cq05mqter9099pumlji"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f77908c5-de15-4bd0-8d89-603bd79d38ae"
      }
    },
    "owner": "manideep1428",
    "android": {
      "package": "com.manideep1428.gymupdated",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "NOTIFICATIONS",
        "FOREGROUND_SERVICE",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "notification": {
        "icon": "./assets/images/icon.png",
        "color": "#ffffff",
        "sound": "./assets/notifications.mp3",
        "androidMode": "default",
        "androidCollapsedTitle": "GYM App"
      }
    },
    "notification": {
      "icon": "./assets/images/icon.png",
      "color": "#ffffff",
      "sound": "./assets/notifications.mp3",
      "androidMode": "default",
      "androidCollapsedTitle": "GYM App",
      "iosDisplayInForeground": true
    }
  }
}
